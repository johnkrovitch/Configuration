{
    "name": "johnkrovitch/configuration",
    "description": "Configuration abstract class for Symfony projects",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "JohnKrovitch",
            "email": "arnaudfrezet@gmail.com"
        }
    ],
    "require": {
        "php": "^8.0",
        "symfony/options-resolver": "^5.4 || ^6.0",
        "symfony/dependency-injection": "^5.3 || ^6.0"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^3.0",
        "johnkrovitch/test-pack": "*"
    },
    "autoload": {
        "psr-4": {
            "JK\\Configuration\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4-dev": {
            "JK\\Configuration\\Tests\\": "tests/"
        }
    },
    "config": {
        "bin-dir": "bin"
    }
}
